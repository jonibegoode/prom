{% extends 'base.html.twig' %}

{% block body %}
    <h1>Trialarmfile</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ trialArmFile.id }}</td>
            </tr>
            <tr>
                <th>Armname</th>
                <td>{{ trialArmFile.armName }}</td>
            </tr>
            <tr>
                <th>Armsize</th>
                <td>{{ trialArmFile.armSize }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if trialArmFile.updatedAt %}{{ trialArmFile.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('arm_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('arm_edit', { 'id': trialArmFile.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
